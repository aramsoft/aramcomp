    "http://jakarta.apache.org/commons/dtds/validator_1_1.dtd">

<form-validation>
    <formset>
		<form name="stplatManageVO">

			<field property="useStplatNm" depends="required, maxlength">
        		<arg0 key="stplatManageVO.useStplatNm" resource="true"/>
