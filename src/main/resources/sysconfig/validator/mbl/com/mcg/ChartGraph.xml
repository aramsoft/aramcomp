<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE form-validation PUBLIC 
    "-//Apache Software Foundation//DTD Commons Validator Rules Configuration 1.1//EN" 
    "http://jakarta.apache.org/commons/dtds/validator_1_1.dtd">

<form-validation>
    <formset>
		<form name="chartGraphVO">
		
			<field property="lgdNm" depends="required, maxlength">
        		<arg0 key="범례명" resource="true"/>
        		<arg1 key="60" resource="false"/>
				<var>
					<var-name>maxlength</var-name>
					<var-value>60</var-value>
				</var>        		
			</field>
			
			<field property="xaxis" depends="required, maxlength">
        		<arg0 key="X축 값" resource="true"/>
        		<arg1 key="20" resource="false"/>
				<var>
					<var-name>maxlength</var-name>
					<var-value>20</var-value>
				</var>        		
			</field>
			
			<field property="yaxis" depends="required, integer">
        		<arg0 key="Y축 값" resource="true"/>
			</field>
			
		</form>
    </formset>
</form-validation>